{
  "port": 3000,
  "buildDirectory": "./build",
  "engine": {
    "launchOptions": {
      "args": ["--no-sandbox", "--disable-setuid-sandbox"],
      "product": "chrome",
      "headless": true,
      "ignoreHTTPSErrors": true
    },
    "gotoOptions": {
      "timeout": 0
    },
    "addScriptTag": "window.__REACT_SNAP__ = true"
  },
  "routes": [
    "/",
    "/login",
    "/registration",
    "/offer",
    "/privacy",
    "/prices"
  ]
}