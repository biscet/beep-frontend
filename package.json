{
  "name": "beep-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd farm start --mode development --config config/farm.dev.js",
    "start:ssh": "ssh -R 80:localhost:3005 localhost.run",
    "start:prod": "yarn build && serve -l 3000 -s build",
    "build": "env-cmd farm build --mode production --config config/farm.prod.js",
    "build:vercel": "farm build --mode production --config config/farm.prod.js",
    "build:clear": "node ./scripts/clear-cache-folder.js",
    "build:gzip": "node ./scripts/gzip.js",
    "build:obfuscate": "node ./scripts/obfuscate.js",
    "build:prod": "yarn build && yarn build:clear && yarn build:obfuscate && yarn build:gzip",
    "lint:eslint": "eslint --fix .",
    "lint:stylelint": "stylelint --fix \"**/*.css\" \"**/*.scss\"",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "optimize-image": "node ./scripts/optimizer-images.js",
    "proxy": "env-cmd node ./scripts/argo-proxy-server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "dependencies": {
    "@effector/reflect": "8.3.1",
    "@wavesurfer/react": "1.0.7",
    "@withease/contracts": "1.1.0",
