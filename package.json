{
  "name": "beep-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd farm start --mode development --config config/farm.dev.js",
    "start:prod": "yarn build && serve -s build",
    "start:storybook": "storybook dev -p 6006 --config-dir config/storybook",
    "build": "env-cmd farm build --mode production --config config/farm.prod.js && yarn build:clear",
    "build:storybook": "storybook build --config-dir config/storybook",
    "build:clear": "node ./scripts/clear-cache-folder.js",
    "lint:eslint": "eslint --fix .",
    "lint:stylelint": "stylelint --fix \"**/*.css\" \"**/*.scss\"",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "optimize-image": "node ./scripts/optimizer-images.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 safari version"
    ]
  },
  "dependencies": {
    "axios": "0.27.2",
    "effector": "22.8.5",
    "effector-forms": "1.3.1",
    "effector-react": "22.5.2",
    "framer-motion": "6",
    "patronum": "1.10.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-parallax-mouse": "^2.1.0",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.21.0",
    "@babel/plugin-transform-runtime": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@farmfe/cli": "1.0.2",
    "@farmfe/core": "1.2.6",
    "@farmfe/js-plugin-postcss": "1.7.0",
    "@farmfe/js-plugin-sass": "2.8.1",
    "@farmfe/plugin-react": "1.1.0",
    "@storybook/addon-essentials": "8.2.1",
    "@storybook/addon-links": "8.2.1",
    "@storybook/addons": "^7.6.17",
    "@storybook/blocks": "8.2.1",
    "@storybook/react": "8.2.1",
    "@storybook/react-vite": "8.2.1",
    "@storybook/test": "8.2.1",
    "@storybook/theming": "^8.2.1",
    "autoprefixer": "10.4.14",
    "babel": "6.23.0",
    "env-cmd": "10.1.0",
    "eslint": "8.37.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-perf": "3.3.0",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-unicorn": "46.0.0",
    "fs-extra": "11.2.0",
    "history": "5.0.0",
    "postcss": "8.4.21",
    "postcss-preset-env": "8.3.0",
    "postcss-will-change": "4.0.1",
    "prop-types": "15.8.1",
    "react-refresh": "0.14.2",
    "rollup-plugin-javascript-obfuscator": "1.0.4",
    "sass": "1.50.0",
    "serve": "14.0.1",
    "sharp": "0.33.4",
    "storybook": "8.2.1",
    "stylelint": "13.9.0",
    "stylelint-config-idiomatic-order": "8.1.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-high-performance-animation": "1.5.2",
    "stylelint-scss": "3.18.0",
    "vite": "5.3.3",
    "vite-plugin-eslint": "1.8.1",
    "vite-plugin-sitemap": "0.6.2",
    "vite-plugin-stylelint": "5.3.1"
  }
}
