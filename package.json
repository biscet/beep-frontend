    "@withease/web-api": "1.3.0",
    "axios": "0.27.2",
    "effector": "22.8.5",
    "effector-forms": "1.3.1",
    "effector-react": "22.5.2",
    "effector-storage": "6.0.0",
    "env-cmd": "10.1.0",
    "event-source-polyfill": "1.0.31",
    "framer-motion": "6",
    "javascript-obfuscator": "4.1.1",
    "konva": "9.3.15",
    "odometer": "0.4.8",
    "patronum": "1.10.0",
    "puppeteer": "23.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "14.2.3",
    "react-helmet": "6.1.0",
    "react-konva": "17.0.2-6",
    "react-parallax-mouse": "2.1.0",
    "react-router-dom": "5.2.0",
    "react-toastify": "8.2.0",
    "uuid": "10.0.0",
    "wavesurfer.js": "7.8.6",
    "zlib": "1.0.5"
